    "broccoli-persistent-filter": "^2.1.1",
    "chalk": "~4.1.1",
    "ignore": "^5.0.2",
    "js-string-escape": "~1.0.1",
    "lodash.defaultsdeep": "^4.6.0",
    "stylelint": "^13.2.1"
