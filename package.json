{
  "name": "broccoli-stylelint",
  "version": "4.0.0",
  "description": "Broccoli stylelint plugin",
  "keywords": [
    "broccoli",
    "broccoli-plugin",
    "stylelint"
  ],
   "engines" : {
     "node" : ">=10.x"
  },
  "main": "index.js",
